{
  "compilerOptions": {
    // "composite": true,
    "strict": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
  ],
  "typeAcquisition": {
    "enable": true
  },
}
